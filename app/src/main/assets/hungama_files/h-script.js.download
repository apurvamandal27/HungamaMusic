function resizePlans() {
    var a = $('.comboOffer').find('ol').eq(0).height();
    $('.cd-container').find('ol').height(a);
    $('.cd-container .comboOffer').find('ol').height('auto');
    /*$('.cd-item-wrapper').each(function(){
     me = $(this).find('.payRs').eq(0).width();
     $(this).find('.payCoins').width(me);
     });*/
    $('.payRs').each(function () {
        me = $(this).width();
        $(this).next('.payCoins').width(me);
    });
}


