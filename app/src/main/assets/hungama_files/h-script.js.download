/* New language bar dated 25-01-2018 */
$(document).ready(function(){
	var language_options = {
		loop:false,
		margin:0,
		slideBy:1,
		items: 3,
		stagePadding: 30,
		nav:false,
		responsiveClass:true,
		lazyLoad:false,
		lazyContent:false,
		responsive:{
			0:{items:2},
			400:{items:3},
			500:{items:4},
			600:{items:5},
			700:{items:6},
			800:{items:7},
			900:{items:8},
			1000:{items:9}
		}
	};
	if( isTouchDevice ) {				
		$('.language-carousal').owlCarousel(language_options);
	}else{
		$('.language-carousal').owlCarousel('destroy');
		$('.language-carousal').show();
	}
	$( window ).resize(function() {
		if( isTouchDevice ) {	
			$('.language-carousal').owlCarousel('destroy');			
			$('.language-carousal').owlCarousel(language_options);
		}else{
			$('.language-carousal').owlCarousel('destroy');
			$('.language-carousal').show();
		}
	});
});